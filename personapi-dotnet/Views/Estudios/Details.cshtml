@model personapi_dotnet.Models.Entities.Estudio

@{
    ViewData["Title"] = "Detalles Registro de Formación Académica";
}

<h1>Detalles Registro de Formación Académica</h1>

<div>
    <h4>Estudios:</h4>
    <hr />
    <dl class="row">
        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Fecha)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Fecha)
        </dd>

        <dt class="col-sm-2">
            @Html.DisplayNameFor(model => model.Univer)
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.Univer)
        </dd>
        <dt class="col-sm-2">
            Persona
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.CcPerNavigation.Cc) - @Html.DisplayFor(model => model.CcPerNavigation.Nombre) @Html.DisplayFor(model => model.CcPerNavigation.Apellido)
        </dd>
        <dt class="col-sm-2">
            Profesión
        </dt>
        <dd class="col-sm-10">
            @Html.DisplayFor(model => model.IdProfNavigation.Id) - @Html.DisplayFor(model => model.IdProfNavigation.Nom)
        </dd>
    </dl>
</div>

<div>
    <a asp-action="Index">Volver a la lista</a>
</div>
