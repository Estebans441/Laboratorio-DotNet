@model IEnumerable<personapi_dotnet.Models.Entities.Telefono>

@{
    ViewData["Title"] = "Index";
}

<h1>Registro telefónico de personas:</h1>

<p>
    <a asp-action="Create">Crear Registro</a>
</p>
<table class="table">
    <thead>
        <tr>
            <th>
                @Html.DisplayNameFor(model => model.Num)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.Oper)
            </th>
            <th>
                @Html.DisplayNameFor(model => model.DuenioNavigation.Nombre)
            </th>
            <th></th>
        </tr>
    </thead>
    <tbody>
@foreach (var item in Model) {
        <tr>
            <td>
				@Html.DisplayFor(modelItem => item.Num)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.Oper)
            </td>
            <td>
                @Html.DisplayFor(modelItem => item.DuenioNavigation.Cc)
            </td>
            <td>
                <a asp-action="Editar" asp-route-id="@item.Num">Edit</a> |
                <a asp-action="Detalles" asp-route-id="@item.Num">Details</a> |
                <a asp-action="Eliminar" asp-route-id="@item.Num">Delete</a>
            </td>
        </tr>
}
    </tbody>
</table>
